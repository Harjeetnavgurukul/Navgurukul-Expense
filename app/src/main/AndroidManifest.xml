<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.navgurukul.hisaab.Expense">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LandingPage" />
        <activity android:name=".User_Status" />
        <activity android:name=".RequestBillPayment" />
        <activity android:name=".RequestTransfer" />
        <activity android:name=".ViewPayments" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ForgotPassword"></activity>
    </application>

</manifest>